class TestImg3 {
    
    main2() {
        def a = 10;
        def b = 15;
        def c = 5;
        def d = a + b + 7; // --> d - 1*(a+b) > 7
        for (def i = 0; i < a; i = i + 1) {
            // d - a - b > 2 --> d - 1*(a+b)>2
            if (d > a + b + 2) {
                c = 0;
            } else {
                c = 1;
            }
        }
    }
}
